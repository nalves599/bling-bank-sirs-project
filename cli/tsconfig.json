{
  "compilerOptions": {
      "module": "commonjs",
      "moduleResolution": "node",
      "sourceMap": true,
      "target": "es2020",
      "declaration": true,
      "outDir": "./dist",
      "strict": true,
      "paths": {
        "@/*": ["./src/*"],
        "@@/*": ["../../library/src/*"],
      }
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules","dist"]
}
